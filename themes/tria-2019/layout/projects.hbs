<h1>{{page.title}}</h1>
<ul class="projects">
  {{#each site.data.projects}}
    <li>
      <h2>{{name}}</h2>
      <p>{{description}}</p>
      {{#if links.length}}
        <nav aria-labelledby="{{id}}-links">
          <h3 id="{{id}}-links">Links</h3>
          {{#each links}}
            <a href="{{link}}">{{type}}</a>
          {{/each}}
        </nav>
      {{/if}}
    </li>
  {{/each}}
</ul>
